    <actions>
    </actions>
    <widgets>
        <container-row>
            <row-col lg="6" md="6">
                <container-box>
                    <box-header><label text="Usage" type="h5"/></box-header>
                    <box-body>
                        <link url="../ArtifactHitSummary" text="Artifact Hit Summary"/>
                        <link url="../ArtifactHitBins" text="Artifact Hit Bins"/>
                        <link url="../AuditLog" text="Audit Log"/>
                        <link url="../Visit" text="Visits"/>
                    </box-body>
                </container-box>
                <container-box>
                    <box-header><label text="Security" type="h5"/></box-header>
                    <box-body>
                        <link url="../Security/UserAccount" text="Users"/>
                        <link url="../Security/UserGroup" text="User Groups"/>
                        <link url="../Security/ArtifactGroup" text="Artifact Groups"/>
                    </box-body>
                </container-box>
            </row-col>
            <row-col lg="6" md="6">
                <container-box>
                    <box-header><label text="Server Admin" type="h5"/></box-header>
                    <box-body>
                        <link url="../Cache" text="Cache Stats and Mgmt"/>
                        <link url="../Scheduler" text="Job Scheduler"/>
